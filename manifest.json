{
  "manifest_version": 2,

  "name": "Night Mode - Alfa Version",
  "author": "Felipe A. L. Reis",
  "description": "This extension allows the user view the page in night mode.",
  "version": "0.1",

  "icons":{
    "128": "favicon.png"
  },
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "homepage_url": "http://falreis.eng.br",
  "short_name": "Night Mode",
  "version_name": "FALR-NM-A01"
}
